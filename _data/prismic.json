            },
            {
              "type": "case-study-text",
              "value": [
                {
                  "title": {
                    "type": "Text",
                    "value": "An idea that was destined to fail..."
                  },
                  "text": {
                    "type": "Text",
                    "value": "We developed an online series about a Volkswagen dealer given an impossible task. The premise was to sell the ultimate city car in an outback town filled with folks with no want or need for it."
                  },
                  "background-style": {
                    "type": "Select",
                    "value": "white"
                  },
                  "mobile-aspect-ratio": {
                    "type": "Select",
                    "value": "1-1"
